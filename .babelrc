{
  "presets": ["es2015","react", "env", "stage-0"],
  "plugins": [
    [
      "transform-runtime", {  "helpers": false, "polyfill": true, "regenerator": true }
    ],
    [
      "import", { "libraryName": "antd-mobile", "style": true}
    ],
    "transform-decorators-legacy",
    "add-module-exports"
  ]
}